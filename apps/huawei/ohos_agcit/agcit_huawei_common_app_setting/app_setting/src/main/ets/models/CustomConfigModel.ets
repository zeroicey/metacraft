import { DisplayType } from '../types/SettingType';

// 参数配置模型
@ObservedV2
export class CustomConfigModel {
  // 路由栈
  @Trace pathStack: NavPathStack = new NavPathStack();
  // 展示形式
  @Trace displayType: DisplayType = DisplayType.PAGE;
  // 主题色
  @Trace themeColor: ResourceColor = $r('sys.color.brand');
  // 相关协议
  @Trace agreementModel: AgreementModel = new AgreementModel();
  // 关于我们
  @Trace aboutModel: AboutModel = new AboutModel();

  // 是否半模态
  @Computed
  get isSheet() {
    return this.displayType === DisplayType.SHEET;
  }
}

